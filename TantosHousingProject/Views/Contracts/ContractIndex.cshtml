@model List<Contract>
@{
    ViewData["Title"] = "Contract Index";

}

<table>
    <thead>
        <tr>
            <th>Room Number</th>
            <th>Tenant Name</th>
            <th>Room Price</th>
            <th>Payment Date</th>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.RoomInQuestion.RoomNumber</td>
            <td>@item.TenantInQuestion.TenantName</td>
            <td>@item.RoomPrice</td>
            <td>@item.PaymentDate</td>
            <td>@item.StartDate</td>
            <td>@item.EndDate</td>
            <td></td>
        </tr>
        }
    </tbody>
</table>