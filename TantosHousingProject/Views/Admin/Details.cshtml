@model IdentityUser
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<dl>
    <dt>User name</dt>
    <dd>@Model.UserName</dd>

    <dt>Email address</dt>
    <dd>@Model.Email</dd>

    <dt>Phone Number</dt>
    <dd>@Model.PhoneNumber</dd>

    <dt>Lock-out Enabled</dt>
    <dd>@Model.LockoutEnabled</dd>

    <dt>Actions</dt>
    <dd>@Html.ActionLink("Roles Management", "RolesManagement", "Admin", new { id = Model.Id})</dd>

</dl>


